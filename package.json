    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.7.4",
    "autoprefixer": "^10.4.20",
    "bits-ui": "1.0.0-next.64",
    "clsx": "^2.1.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "^15.0.0",
    "lucide-svelte": "^0.446.0",
    "mdsvex": "^0.12.3",
    "postcss": "^8.4.47",
