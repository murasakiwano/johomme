  "type": "module",
  "dependencies": {
    "gemoji": "^8.1.0",
    "mode-watcher": "^0.4.1",
    "rehype-external-links": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
